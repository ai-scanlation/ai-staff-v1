{
    "name": "ai.staff",
    "version": "2.0.1",
    "description": "Ai Scanlation Tool for Staff",
    "author": "phamhongphuc <phamhongphuc.info@gmail.com>",
    "private": true,
    "scripts": {
        "go": "electron-webpack dev",
        "style": "scss --watch style/index.scss:dist/render.css",
        "style-render": "scss --style compressed style/index.scss:dist/render.css",
        "webpack": "webpack --watch",
        "webpack-render": "webpack",
        "electron": "electron .",
        "electron-debug": "electron --debug-brk=5858 .",
        "electron-render": "node renderer.js",
        "dev": "cross-env NODE_ENV=development npm-run-all --parallel style webpack electron",
        "debug": "cross-env NODE_ENV=development npm-run-all --parallel style webpack electron-debug",
        "build": "cross-env NODE_ENV=production run-s style-render webpack-render",
        "render": "cross-env NODE_ENV=production npm run electron-render"
    },
    "main": "app/main.js",
    "electronWebpack": {
        "main": {
            "sourceDirectory": "app"
        },
        "renderer": {
            "sourceDirectory": "script",
            "webpackConfig": "main.config.js"
        }
    },
    "dependencies": {
        "animejs": "^2.0.2",
        "axios": "^0.17.1",
        "chart.js": "^2.6.0",
        "express": "^4.16.2",
